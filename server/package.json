    "dotenv": "^16.3.1",
    "firebase-admin": "^11.11.1",
    "retell-sdk": "^2.3.0",
    "openai": "^4.28.0",
    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "express-rate-limit": "^7.1.5",
